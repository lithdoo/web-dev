{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Users",
  "description": "System users table",
  "properties": {
    "id": {
      "type": "string",
      "description": "User unique identifier"
    },
    "name": {
      "type": "string",
      "description": "User name"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "User email"
    }
  },
  "required": [
    "id",
    "name",
    "email"
  ]
}
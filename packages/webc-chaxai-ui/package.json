{
  "name": "webc-chaxai-ui",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist",
    "src/components"
  ],
  "main": "./dist/webc-file-view.umd.js",
  "module": "./dist/webc-file-view.es.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/webc-file-view.es.js",
      "require": "./dist/webc-file-view.umd.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vue-tsc -b && vite build",
    "build:wc": "vue-tsc -b && vite build && vite build --config vite.config.wc.ts"
  },
  "peerDependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "sass-embedded": "^1.97.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  }
}
